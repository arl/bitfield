# Control the total number of bits for an union is < 65
! bitfield -in $WORK/in.go
! stdout .
cp stderr got
cmp got want.1

-- in.go --
package main

type MyStruct struct {
	CoarseX    uint8  `bitfield:"bits=5"`
	CoarseY    uint8  `bitfield:"bits=5"`
	NametableX uint8  `bitfield:"bits=1"`
	NametableY uint8  `bitfield:"bits=1"`
	FineY      uint8  `bitfield:"bits=3"`
	_          uint8  `bitfield:"bits=1"`
	_          uint16 `bitfield:"bits=16"`
	BigField   uint32 `bitfield:"bits=32"`

	SuperBigField uint64 `bitfield:"bits=64,union=a"`
	EigthBits     uint64 `bitfield:"bits=8,union=a"`
}

-- want.1 --
struct 'MyStruct' has too many bits in union 'a' (72)